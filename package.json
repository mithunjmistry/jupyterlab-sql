{
  "name": "jupyterlab-sql",
  "version": "0.3.3",
  "description": "JupyterLab extension for interacting with SQL databases",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "github.com/pbugnion/jupyterlab-sql",
  "bugs": {
    "url": "github.com/pbugnion/jupyterlab-sql/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Pascal Bugnion",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "github.com/pbugnion/jupyterlab-sql.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:dist": "npm run prepare && rimraf labextension && mkdirp labextension && cd labextension && npm pack ..",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab_sql/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "prepare": "npm runtall clean && npm run build",
    "prettier": "prettier --write '{src,__tests__}/**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "prettier:check": "prettier --list-different '{src,__tests__}/**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "jest",
    "test:changed": "jest -o",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@jupyterlab/application": "^3.0.9",
    "@jupyterlab/codeeditor": "^3.0.7",
    "@jupyterlab/launcher": "^3.0.7",
    "@jupyterlab/testutils": "^2.1.2",
    "@lumino/datagrid": "^0.7.2",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.16.1",
    "@types/node": "^14.0.1",
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sql-formatter": "^4.0.2",
    "url-parse": "^1.4.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@jupyterlab/builder": "^3.0.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.9.1",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^3.4.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.1.0",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "jest-fetch-mock": "^2.1.2",
    "mkdirp": "^0.5.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.3.0",
    "typescript": "~4.1.3"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_sql/labextension"
  },
  "styleModule": "style/index.js"
}